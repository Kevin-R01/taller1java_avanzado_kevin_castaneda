<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>Usuario</servlet-name>
    <servlet-class>controller.Usuario</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Usuario</servlet-name>
    <url-pattern>/Usuario</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Actualizar</servlet-name>
    <servlet-class>main.java.controller.Actualizar</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>Actualizar</servlet-name>
    <url-pattern>/Actualizar</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>PublicacionController</servlet-name>
    <servlet-class>main.java.controller.PublicacionController</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>PublicacionController</servlet-name>
    <url-pattern>/guardarPublicacion</url-pattern>
</servlet-mapping>


    <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>controller.Login</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>

    <servlet>
    <servlet-name>EliminarUsuario</servlet-name>
    <servlet-class>main.java.controller.EliminarUsuario</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>EliminarUsuario</servlet-name>
    <url-pattern>/EliminarUsuario</url-pattern>
</servlet-mapping>
  
</web-app>
